<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="group1.android.calculatorkit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalculatorKit">
        <activity android:name=".AreaSurfaceAreaAndVolumeMeasureActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".CircleAndLineIntersectionActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".IntersectionActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".CircleIntersectionActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleCalculatorActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".GeometryCalculatorActivity" />
        <activity android:name=".MatrixCalculatorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ConversionCalculatorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".AreaSurfaceAreaAndVolumeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".LineIntersectionActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".DistanceActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".AnglesActivity" />
        <activity android:name=".AnglesTriangleActivity" />
        <activity android:name=".AnglesQuadrilateralActivity" />
        <activity android:name=".AnglesPentagonActivity" />
        <activity android:name=".AnglesHexagonActivity" />
    </application>

</manifest>